
<!DOCTYPE html>
<html>
  <head>
    <title>show scores</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
    </script>
  </head>
  <body>
    <h1>show scores</h1>
    <table>
    <tr>
      
<th>score</th>
      
<th>type</th>
      
<th>date</th>
      
<th>score_id</th>
      
<th>user_id</th>
      
<th>user_name</th>
      
      </tr>
    <% for(var i=0;i < docs.length;i++) { %>      
    <tr>
      
    <td><%=docs[i].score%></td>      
    <td><%=docs[i].type%></td>      
    <td><%=docs[i].date%></td>      
    <td><%=docs[i].score_id%></td>      
    <td><%=docs[i].user_id%></td>      
    <td><%=docs[i].user_name%></td>      
    <td><a href="/score/update/?id=<%=docs[i]._id%>">edit</a></td>    </tr>
    <% } %>    </table>
      <a href="/score/create">create new</a>
  </body>
</html>
